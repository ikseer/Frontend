{
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"target": "ES2017",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": true
	}
}
