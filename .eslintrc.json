{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-var":"error",
    "quotes": ["error", "single"],
    "camelcase": ["error", { "properties": "never" }],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }],
    "indent": ["error", 2, {"SwitchCase": 1}]
  }
}
